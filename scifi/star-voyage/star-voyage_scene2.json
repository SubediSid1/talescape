      "character": {
        "name": "Alex",
        "image": "https://ix-marketing.imgix.net/focalpoint.png"
      },
      "isUser": true
    },
    {
      "chapter_id": "ch48",
      "text": "Locked, but I can bypass it. Give me a sec.",
      "content_image": null,
      "character": {
        "name": "Bob",
        "image": "https://ix-marketing.imgix.net/footer-image.png"
      },
      "isUser": false
    },
    {
      "chapter_id": "ch49",
      "text": "Hurry up. That hum’s getting louder.",
