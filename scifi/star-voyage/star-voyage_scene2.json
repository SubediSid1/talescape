      "character": {
        "name": "Alex",
        "image": "https://ix-marketing.imgix.net/focalpoint.png"
      },
      "isUser": true
    },
    {
      "chapter_id": "ch40",
      "text": "Follow the cables on the wall. They’ll lead us somewhere.",
      "content_image": null,
      "character": {
        "name": "Bob",
        "image": "https://ix-marketing.imgix.net/footer-image.png"
      },
      "isUser": false
    },
    {
      "chapter_id": "ch41",
      "text": "Smart. This place is giving me the chills.",
