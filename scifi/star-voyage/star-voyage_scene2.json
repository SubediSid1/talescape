      "character": {
        "name": "Alex",
        "image": "https://ix-marketing.imgix.net/focalpoint.png"
      },
      "isUser": true
    },
    {
      "chapter_id": "ch50",
      "text": "Got it! Door’s open—control room’s inside.",
      "content_image": null,
      "character": {
        "name": "Bob",
        "image": "https://ix-marketing.imgix.net/footer-image.png"
      },
      "isUser": false
